        "moment-timezone": "^0.6.0",
        "mysql": "^2.18.1",
        "mysql2": "^3.16.0",
        "pg": "^8.16.3",
        "puppeteer": "^24.34.0",
        "request": "^2.88.2",
        "sharp": "^0.34.5",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
