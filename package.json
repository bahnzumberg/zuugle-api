        "@types/express": "^5.0.6",
        "@types/node": "^25.0.3",
        "babel-jest": "^30.2.0",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "globals": "^17.0.0",
        "husky": "^9.1.7",
        "jest": "^30.2.0",
        "lint-staged": "^16.2.7",
        "nodemon": "^3.1.11",
        "prettier": "^3.7.4",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.51.0"
    },
