{
    "version": "1.0",
    "patterns": {
        "ascent": [
            {
                "regex": "max(?:imal)?\\s*(\\d+)\\s*(?:hm|höhenmeter)",
                "extract": "maxAscent",
                "type": "number",
                "captureGroup": 1
            },
            {
                "regex": "mindestens\\s*(\\d+)\\s*(?:hm|höhenmeter)",
                "extract": "minAscent",
                "type": "number",
                "captureGroup": 1
            },
            {
                "regex": "(\\d+)\\s*bis\\s*(\\d+)\\s*(?:hm|höhenmeter)",
                "extract": ["minAscent", "maxAscent"],
                "type": "range",
                "captureGroups": [1, 2]
            },
            {
                "regex": "zwischen\\s*(\\d+)\\s*und\\s*(\\d+)\\s*(?:hm|höhenmeter|meter)",
                "extract": ["minAscent", "maxAscent"],
                "type": "range",
                "captureGroups": [1, 2]
            },
            {
                "regex": "aufstieg\\s*(\\d+)\\s*m",
                "extract": "minAscent",
                "type": "number",
                "captureGroup": 1
            },
            {
                "regex": "(\\d+)\\s*m\\s*aufstieg",
                "extract": "minAscent",
                "type": "number",
                "captureGroup": 1
            },
            { "regex": "(\\d+)\\s*hm", "extract": "maxAscent", "type": "number", "captureGroup": 1 }
        ],
        "duration": [
            {
                "regex": "(\\d+)\\s*tage(?:stour)?",
                "extract": "number_of_days",
                "type": "number",
                "captureGroup": 1,
                "sideEffect": { "multipleDayTour": true }
            },
            {
                "regex": "(\\d+)\\s*bis\\s*(\\d+)\\s*(?:h|stunden?)",
                "extract": ["minDuration", "maxDuration"],
                "type": "range",
                "captureGroups": [1, 2]
            },
            {
                "regex": "(?:dauer|ca\\.?)\\s*(\\d+)\\s*(?:h|stunden?)",
                "extract": "maxDuration",
                "type": "number",
                "captureGroup": 1
            },
            {
                "regex": "kurze\\s*tour(?:en)?",
                "extract": "maxDuration",
                "type": "literal",
                "value": 3
            }
        ],
        "travelTime": [
            {
                "regex": "anreise.*?(?:max(?:imal)?)?\\s*(\\d+)\\s*(?:h|stunden?)",
                "extract": "maxTransportDuration",
                "type": "number",
                "captureGroup": 1
            },
            {
                "regex": "(?:in|ungefähr)\\s*(\\d+)\\s*(?:h|stunde).*?erreichbar",
                "extract": "maxTransportDuration",
                "type": "number",
                "captureGroup": 1
            },
            {
                "regex": "(\\d+)\\s*(?:h|stunde)\\s*(?:von|ab)\\s+\\w+",
                "extract": "maxTransportDuration",
                "type": "number",
                "captureGroup": 1
            }
        ]
    },
